  "scripts": {
    "build": "node scripts/build.js",
    "start": "node scripts/start.js",
    "test": "jest --watch",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "build:analyze": "build && analyze",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "doctor": "doctor",
    "e2e": "start-server-and-test start http://localhost:5001 cypress",
    "e2e:headless": "start-server-and-test start http://localhost:5001 cypress:headless",
    "eslint": "eslint --config ./.eslintrc.js --resolve-plugins-relative-to ./ --ext .js,.json,.jsx,.ts,.tsx \"./src\"",
    "eslint:all": "eslint --config ./.eslintrc.js --resolve-plugins-relative-to ./ --ext .js,.json,.jsx,.ts,.tsx \".\"",
    "eslint:fix": "eslint --fix --config ./.eslintrc.js --resolve-plugins-relative-to ./ --ext .js,.json,.jsx,.ts,.tsx \"./src\"",
    "eslint:fix:all": "eslint --fix --config ./.eslintrc.js --resolve-plugins-relative-to ./ --ext .js,.json,.jsx,.ts,.tsx \".\"",
    "prepare": "husky install",
    "prettier": "prettier --check \"src/**/*.{css,html,js,json,jsx,less,markdown,md,mdx,sass,scss,ts,tsx,vue,yaml,yml}\"",
    "prettier:all": "prettier --check \"**/*.{css,html,js,json,jsx,less,markdown,md,mdx,sass,scss,ts,tsx,vue,yaml,yml}\"",
    "prettier:write": "prettier --write \"src/**/*.{css,html,js,json,jsx,less,markdown,md,mdx,sass,scss,ts,tsx,vue,yaml,yml}\"",
    "prettier:write:all": "prettier --write \"**/*.{css,html,js,json,jsx,less,markdown,md,mdx,sass,scss,ts,tsx,vue,yaml,yml}\"",
    "sdks": "sdks vscode",
    "serve": "serve -s build",
    "server": "node server/server.js",
    "stylelint": "stylelint --config ./.stylelintrc.json \"**/*.{css,less,sass,scss}\"",
    "stylelint:fix": "stylelint --fix --config ./.stylelintrc.json \"**/*.{css,less,sass,scss}\"",
    "test:ci": "jest --ci",
    "update:check": "npm-check-updates --color --concurrency 8 --configFilePath . --configFileName .ncurc.json --cwd . --dep prod,dev,bundle,optional,peer --errorLevel 1",
    "update:upgrade": "npm-check-updates --upgrade --color --concurrency 8 --configFilePath . --configFileName .ncurc.json --cwd . --dep prod,dev,bundle,optional,peer --errorLevel 1",
    "heroku-prebuild": "",
    "heroku-postbuild": "node scripts/build.js",
    "heroku-cleanup": ""
  },
