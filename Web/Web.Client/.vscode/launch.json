{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug (.env.local)",
      "request": "launch",
      "type": "pwa-node",
      "console": "integratedTerminal",
      "runtimeExecutable": "yarn",
      "runtimeArgs": [
        "start"
      ],
      "skipFiles": [
        "<node_internals>/**"
      ],
      "envFile": "${workspaceFolder}/.env.local",
      "localRoot": "${workspaceFolder}",
      "cwd": "${workspaceFolder}"
    },
